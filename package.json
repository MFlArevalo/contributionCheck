{
  "name": "singledraft-word-plugin",
  "version": "0.0.1",
  "config": {
    "app_to_debug": "word",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:staging": "webpack --mode production --env.NODE_ENV=staging",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "start": "office-addin-debugging start manifest.xml",
    "start:desktop": "npm run stop && office-addin-debugging start manifest.xml desktop",
    "start:web": "npm run stop && office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate-manifest": "office-addin-manifest validate manifest.xml",
    "watch": "webpack --mode development --watch",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "cypress-open": "cypress open",
    "prepare": "husky",
    "validate": "lint-staged",
    "eslint": "eslint --fix --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "check-format": "npm run prettier -- --list-different",
    "i18n:sync": "sync-i18n --files **/translations/*.json --primary en --languages en cs de --space 2",
    "i18n:check": "npm run i18n:sync -- --check"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@fluentui/react": "^8.117.1",
    "@fluentui/react-components": "^9.30.4",
    "@fluentui/react-icons": "^2.0.240",
    "@headlessui/react": "^2.0.3",
    "@heroicons/react": "^2.1.3",
    "@hookform/resolvers": "^3.3.4",
    "@microsoft/office-js": "^1.1.91",
    "@storybook/react": "^8.0.5",
    "@tailwindcss/typography": "^0.5.12",
    "@tanstack/react-query": "^5.45.0",
    "@tanstack/react-query-devtools": "^5.45.0",
    "axios": "^1.6.8",
    "cheerio": "^1.0.0-rc.12",
    "classnames": "^2.5.1",
    "clsx": "^2.1.0",
    "core-js": "^3.9.1",
    "dayjs": "^1.11.10",
    "dompurify": "^3.1.3",
    "es6-promise": "^4.2.8",
    "immer": "^10.1.1",
    "jwt-decode": "^4.0.0",
    "lint-staged": "^15.2.2",
    "nanoid": "^5.0.6",
    "pkce-challenge": "^4.1.0",
    "re-resizable": "^6.9.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.51.2",
    "react-i18next": "^14.1.0",
    "react-loading-skeleton": "^3.4.0",
    "react-router-dom": "^6.22.3",
    "react-tooltip": "^5.26.4",
    "regenerator-runtime": "^0.14.1",
    "rehype-react": "^8.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "type-fest": "^4.15.0",
    "unified": "^11.0.4",
    "use-debounce": "^10.0.0",
    "uuid": "^10.0.0",
    "web-vitals": "^4.0.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.13.0",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20.11.28",
    "@types/office-js": "^1.0.377",
    "@types/office-runtime": "^1.0.23",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.23",
    "@types/webpack": "^5.28.5",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.1",
    "cypress": "^13.7.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-jest-dom": "^5.2.0",
    "eslint-plugin-office-addins": "^3.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^9.0.11",
    "i18next-json-sync": "^3.1.2",
    "office-addin-cli": "^1.5.5",
    "office-addin-debugging": "^5.1.2",
    "office-addin-dev-certs": "^1.11.3",
    "office-addin-lint": "^2.2.5",
    "office-addin-manifest": "^1.12.3",
    "office-addin-prettier-config": "^1.2.0",
    "os-browserify": "^0.3.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.5.4",
    "prettier": "^3.2.5",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.3",
    "ts-loader": "^9.4.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "prettier": "office-addin-prettier-config",
  "browserslist": [
    "ie 11"
  ],
  "lint-staged": {
    "*.+(ts|tsx)": [
      "npm run eslint"
    ]
  }
}
